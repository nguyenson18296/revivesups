{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "rejected"
      ],
      "default": "pending"
    },
    "order_line_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-line-item.order-line-item",
      "mappedBy": "order"
    },
    "email": {
      "type": "email"
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "phone_number": {
      "type": "biginteger",
      "required": true
    },
    "detail": {
      "type": "string"
    }
  }
}
